
{%  if home %}
<title>{{ site.name }}</title>
<meta name="twitter:title" content="{{ site.name }}" />
{%  else %}
<title>{{ title }} - {{ site.name }}</title>
<meta name="twitter:title" content="{{ title }} - {{ site.name }}" />
{% endif %}

<meta property="og:site_name" content="heapspace" />
<meta property="og:type" content="website" />
<meta property="og:url" content="{{ site.baseURL }}{{ url }}" />
<meta name="description" content="{{ description}}" />
<meta property="og:description" content="{{ description }}" />
<meta property="twitter:description" content="{{ description }}" />

{% if post %}
  <meta property="og:type" content="article" />
{% else %}
  <meta property="og:type" content="website" />
  <meta property="og:image" content="{{ site.baseURL }}hs-og.png" />
{% endif %}
{#{{ if .Params.post }}#}
{#	<meta property="og:image" content="{{ site.BaseURL }}blog/{{ slug }}/{{ postImage }}" />#}
{#	<meta name="twitter:description" content="{{ .Summary }}" />#}
{#{{ else }}#}
{#	#}
{#{{ end }}#}
<meta name="twitter:card" content="summary_large_image" />

